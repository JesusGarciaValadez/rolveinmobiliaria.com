{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.1.3",
        "algolia/algoliasearch-client-php": "^2.2",
        "bensampo/laravel-enum": "^1.4",
        "doctrine/dbal": "^2.6",
        "fideloper/proxy": "~4.0",
        "laravel/framework": "5.7.*",
        "laravel/horizon": "^1.2",
        "laravel/passport": "^4.0",
        "laravel/scout": "^4.0",
        "laravel/telescope": "^1.0",
        "laravel/tinker": "^1.0",
        "pusher/pusher-php-server": "~3.0",
        "sentry/sentry-laravel": "^0.11.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.1",
        "beyondcode/laravel-dump-server": "^1.0",
        "codedungeon/phpunit-result-printer": "^0.26.1",
        "facile-it/paraunit": "^0.12.3",
        "filp/whoops": "^2.0",
        "friendsofphp/php-cs-fixer": "^2.13",
        "fzaninotto/faker": "^1.4",
        "laravel/dusk": "^3.0",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0",
        "phpunit/phpunit": "^7.0",
        "sensiolabs/security-checker": "^5.0",
        "symfony/thanks": "^1.0",
        "themsaid/laravel-mail-preview": "^2.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "test": [
            "@lint:check",
            "@phpunit",
            "@security"
        ],
        "phpunit": [
            "vendor/bin/phpunit"
        ],
        "security": [
            "vendor/bin/security-checker security:check"
        ],
        "lint": "php-cs-fixer fix --verbose --show-progress=estimating",
        "lint:check": "php-cs-fixer fix --dry-run --verbose --show-progress=estimating"
    }
}
